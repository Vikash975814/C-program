#include<stdio.h>
#include<conio.h>
void main()
{
    int A[3][3],i,j,sum=0;
printf("Enter the element of  Array");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
scanf("%d",&A[i][j]);
        }
    }
printf("Your 2D Array is:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
printf("%d ",A[i][j]);
        }
printf("\n");
    }
    for(i=0;i<3;i++)
    {
        sum=sum+A[i][i];
    }
printf("sum of diogonal element is:%d",sum);
getch();
}




