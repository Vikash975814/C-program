#include<stdio.h>
#include<conio.h>
void main()
{
    int A[3][3],i,j;
printf("Enter the element of  Array");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
scanf("%d",&A[i][j]);
        }
    }

printf("Your 2D Array is:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
printf("%d ",A[i][j]);
        }
printf("\n");
    }
printf("Transpose of Given array\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
printf("%d ",A[j][i]);
        }
printf("\n");
    }
getch();
}
