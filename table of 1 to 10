#include <stdio.h>
#include<conio.h>
void main()  
{  
    int n, i,j; 
    printf (" Enter a number to generate the table in C: ");  
    scanf (" %d", &n);   
    printf ("\n Table of %d", n);
    for ( i = 1; i <= 10; i++)  
    {  
        printf ("\n %d * %d = %d", n, i, (n*i));  
    }  
getch();
} 


