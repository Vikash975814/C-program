#include<stdio.h>
#include<conio.h>
int main()
{
    int var1,var2,temp;
    printf("enter two integers values");
    scanf("%d%d",&var1,&var2);
    printf("before swapping firstvariable=%d second variable=%d",var1,var2);
    temp=var1;
    var1=var2;
    var2=temp;
    printf("after swapping first variable=%d second variable =%d",var1,var2);
    getch();
    return 0;
}
