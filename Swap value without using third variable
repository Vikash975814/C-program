#include<stdio.h>
#include<conio.h>
int main()
{
    int var1,var2;
    printf("enter the two integers values");
    scanf("%d%d",&var1,&var2);
    printf("before swap first var=%d second var=%d",var1,var2);
    var1= var1+var2;
    var2=var1-var2;
    var1=var1-var2;
    printf("after swap first var=%d second var=%d",var1,var2);
    getch();
    return 0;
}
