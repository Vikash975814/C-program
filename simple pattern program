
       #include<stdio.h>
       #include<conio.h>
      void main()
      {
           int n,i,j;
          printf("Enter How many lines you are interested");
          scanf("%d",&n);
           for(i=0;i<=n;i++)
             {
           for(j=1;j<=i;j++)
          {
           printf("%d   ",j);
    }
          printf("\n");
    }
    getch();
}
