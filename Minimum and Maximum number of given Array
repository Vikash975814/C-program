#include<stdio.h>
#include<conio.h>
void main()
{
    int A[100],i,n,min,max;
    printf("How many element do you want to Array");
    scanf("%d",&n);
    printf("Enter the element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&A[i]);
    }
    min=max=A[0];
    for(i=0;i<n;i++)
    {
        if(min>=A[i])
        min=A[i];
        if(max<=A[i])
        max=A[i];
    }
    printf("Min Element is:%d\n",min);
    printf("Max Element is:%d",max);
    getch();
}

