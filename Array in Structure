#include<stdio.h>
#include<conio.h>
void main()
{
struct student
{
int roll;
char name[30];
int age;
};
struct student std[10];
int i;
clrscr();
printf("Enter 10 student record");
printf("\n");
for(i=0;i<10;i++)
{
printf("enter student data");
scanf("%d%s%d",&std[i].roll,&std[i].name,&std[i].age);
}
for(i=0;i<10;i++)
{
printf("%d %s %d\n",std[i].roll,std[i].name,std[i].age);
}
getch();
} 
