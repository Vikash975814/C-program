
#include<stdio.h>
#include<conio.h>
void main()
{	
    int A1[100],A2[100],A3[100],i,n;
    printf("How many element do you want to Array");
    scanf("%d",&n);
    printf("Enter the first Array element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&A1[i]);
    }
    printf("Enter the Second Array element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&A2[i]);
    }
    printf("Sum of Element is Given Both Array");
    printf("\n");
    for(i=0;i<n;i++)
    {
        A3[i]=A1[i]+A2[i];
    printf("%d\n",A3[i]);
    }
getch();
}


