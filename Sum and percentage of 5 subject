 
  #include<stdio.h>
#include<conio.h>
#include<math.h>
int main()
{
int m1,m2,m3,m4,m5;
float sum,per;
printf("enter the 5 subject marks");
scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
sum=(m1+m2+m3+m4+m5);
per=sum/5;
printf("print sum is %f\n",sum);
printf("print percentage is %f",per);
getch();
return 0;
}
